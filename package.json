{
  "name": "react-transparent",
  "version": "1.1.3",
  "description": "Make a component transparent",
  "main": "./dist/umd/index.js",
  "module": "./dist/esm/index.js",
  "scripts": {
    "build": "npm run umd && npm run esm",
    "umd": "babel src -d ./dist/umd",
    "esm": "babel src -d ./dist/esm --no-babelrc --presets=env,react",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weiliy/react-transparent.git"
  },
  "keywords": [
    "react",
    "react-component",
    "component",
    "react-transparent",
    "transparent"
  ],
  "author": "Weili Yi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/weiliy/react-transparent/issues"
  },
  "homepage": "https://github.com/weiliy/react-transparent#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "jest": "^22.1.4",
    "react-test-renderer": "^16.2.0",
    "react": "^16.2.0"
  },
  "peerDependencies": {
    "react": ">= 15"
  },
  "jest": {
    "verbose": true
  }
}
